<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.third.model.CategoryModel" table="category">
		<id name="pk" type="java.lang.String">
			<column name="pk" />
			<generator class="uuid" />
		</id>
		<property column="code" lazy="false" name="code"
			type="java.lang.String" />
		<property column="name" lazy="false" name="name"
			type="java.lang.String" />
	    <property name="modificationTime" update="true" insert="true"
			type="java.util.Date">
			<column name="modification_time" sql-type="timestamp"/>
		</property>
		<property name="createTime" update="false" insert="true"
			type="java.util.Date">
			<column name="create_time" sql-type="timestamp"/>
		</property>
	</class>
</hibernate-mapping>
